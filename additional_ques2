#include <iostream>
using namespace std;

int countBST(int n) {
    if (n == 0 || n == 1) return 1;

    int total = 0;

    for (int i = 1; i <= n; i++) {
        int left = countBST(i - 1);
        int right = countBST(n - i);
        total = total + left * right;
    }

    return total;
}

int main() {
    int n = 3;
    cout << "Number of unique BSTs = " << countBST(n);
}
