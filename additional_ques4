#include <iostream>
using namespace std;

class Node {
public:
    int data;
    Node* left;
    Node* right;

    Node(int x) {
        data = x;
        left = right = NULL;
    }
};

void rightViewUtil(Node* r, int level, int &maxLevel) {
    if (r == NULL) return;

    if (level > maxLevel) {
        cout << r->data << " ";
        maxLevel = level;
    }

    rightViewUtil(r->right, level + 1, maxLevel);
    rightViewUtil(r->left, level + 1, maxLevel);
}

void rightView(Node* root) {
    int maxLevel = 0;
    rightViewUtil(root, 1, maxLevel);
}

int main() {
    Node* root = new Node(1);
    root->left = new Node(2);
    root->right = new Node(3);
    root->left->right = new Node(5);
    root->right->right = new Node(4);

    rightView(root);
}
